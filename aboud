calculate:
la s0, array # *s0 = array[0]
lh s2, 8(s0) # s2 = array[4]
lh s3, 4(s0) # s3 = array[2]
lh s4, 2(s0) # s4 = array[1]
add s2, s2, s3 # s2 = array[4] + array[2]
add s3, s2, s4 # s2 = array[4] + array[2] + array[1]
blt t1, s3, res1 # jump to res1 lable if (array[4] + array[2] + array[1]) > threshold

res2:
lh a3, 12(s0) # a2 = arr[6]
add a5, a3, t2 
j end

res1:
lh a2, 10(s0) # a2 = arr[5]
sub s7, a2, s2 

end:
